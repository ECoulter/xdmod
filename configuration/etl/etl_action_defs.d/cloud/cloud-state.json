{
    "table_definition": {
        "$ref": "${table_definition_dir}/cloud/cloudeventfact.json#/table_definition"
    },
    "destination_record_map": {
        "cloudeventfact": {
            "instance_id": "instance_id",
            "start_time": "start_time",
            "start_event_id": "start_event_id",
            "end_time": "end_time",
            "end_event_id": "end_event_id"
        }
    },
    "source_query": {
        "records": {
            "instance_id": "instance_id",
            "event_time_utc": "event_time_utc",
            "event_type_id": "event_type_id",
            "start_time": -1,
            "start_event_id": -1,
            "end_time": -1,
            "end_event_id": -1
        },
        "joins": [
            {
                "name": "event",
                "schema": "${SOURCE_SCHEMA}",
                "alias": "ev"
            }
        ],
        "orderby": [
            "instance_id ASC, event_time_utc ASC"
        ]
    }
}
